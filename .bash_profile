source ~/.profile
if [ -f ~/.bashalias ]; then
   source ~/.bashalias
fi

if [ -f ~/.bashtp ]; then
   source ~/.bashtp
fi

if [ -f ~/.bashrc ]; then
   source ~/.bashrc
fi
export PATH=/usr/local/mysql/bin:$PATH


# Setting PATH for Python 2.7
# The original version is saved in .bash_profile.pysave
